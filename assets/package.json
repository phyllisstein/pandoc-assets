{
    "name": "pandoc-assets",
    "author": "Daniel P. Shannon <daniel@daniel.sh>",
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "build:dev": "cross-env NODENODE_ENV=development webpack",
        "build:prod": "cross-env NODE_ENV=production webpack && aws --no-cli-pager s3 cp public s3://hot-garb-ag-cloudfront --recursive --exclude .DS_Store --acl bucket-owner-full-control --only-show-errors && aws --no-cli-pager cloudfront create-invalidation --distribution-id E8VRXSVFPL6GI --paths '/*'",
        "clean": "concurrently 'yarn:clean:*'",
        "clean:build": "rimraf public",
        "clean:cache": "rimraf node_modules/.cache",
        "watch": "webpack --watch"
    },
    "dependencies": {
        "@webpack-cli/info": "^1.2.2",
        "@webpack-cli/serve": "^1.3.0",
        "autoprefixer": "^10.2.4",
        "breakpoint-sass": "~2.7.1",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^5.0.2",
        "cssnano": "^4.1.10",
        "fibers": "^5.0.0",
        "file-loader": "^6.2.0",
        "hyphenopoly": "^4.10.0",
        "mathsass": "^0.11.0",
        "mime": "^2.5.2",
        "mini-css-extract-plugin": "^1.3.8",
        "modularscale-sass": "^3.0.10",
        "path-browserify": "^1.0.1",
        "plumber-sass": "^0.7.7",
        "postcss": "^8.2.6",
        "postcss-loader": "^5.0.0",
        "resolve-url-loader": "^4.0.0-alpha.1",
        "sass": "^1.32.8",
        "sass-loader": "^11.0.1",
        "style-loader": "^2.0.0",
        "stylelint": "^13.11.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
        "webpack": "^5.23.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "devDependencies": {
        "@aws-sdk/client-cloudfront": "^3.6.0",
        "@aws-sdk/credential-provider-ini": "^3.6.0",
        "concurrently": "^6.0.0",
        "cross-env": "^7.0.3",
        "rimraf": "^3.0.2"
    }
}
