        "@babel/preset-typescript": "^7.18.6",
        "@github/catalyst": "^1.6.0",
        "@webpack-cli/info": "^1.5.0",
        "@webpack-cli/serve": "^1.7.0",
        "autoprefixer": "^10.4.8",
        "babel-loader": "^8.2.5",
